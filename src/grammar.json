{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "kanata",
  "rules": {
    "kanata": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "list"
      }
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";;"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        }
      ]
    },
    "block_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#|"
        },
        {
          "type": "PATTERN",
          "value": "[^|]*\\|+([^|#][^|]*\\|+)*"
        },
        {
          "type": "STRING",
          "value": "#"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_item"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "unquoted_item"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_item"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        }
      ]
    },
    "unquoted_item": {
      "type": "PATTERN",
      "value": "[^\\s)(\"]+"
    },
    "quoted_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"]*"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    },
    {
      "type": "PATTERN",
      "value": "\\n"
    },
    {
      "type": "PATTERN",
      "value": "\\r"
    },
    {
      "type": "PATTERN",
      "value": "\\p{Zs}"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}