    "license": "MIT",
    "description": "Tree-sitter grammar for kanata",
    "authors": [
      {
        "name": "postsolar",
        "url": "https://github.com/postsolar"
      }
    ],
    "links": {
      "repository": "https://github.com/postsolar/tree-sitter-kanata"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
